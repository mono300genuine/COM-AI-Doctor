{
    "compilerOptions": {
      "allowJs": true,
      "strict": true,
  
      "target": "ESNext",
      "lib": ["ES2021", "dom"],
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["/**/*"],
    "exclude": ["./_generated"]
  }
  